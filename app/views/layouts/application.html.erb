<!DOCTYPE html>
<html>
<head>
  <title>AudioWaterVapor</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if signed_in? %>
  <%= current_user.username %>
<% end  %>
<% if flash[:errors] %>
  <% flash[:errors].each do |error| %>
    <%= error %>
  <% end  %>
<% end  %>


<%= yield %>

</body>
</html>
